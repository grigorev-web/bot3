# ========================================
# LLM API Configuration
# ========================================

# LLM Provider (proxyApi)
LLM_PROVIDER=proxyApi

# ProxyAPI Configuration
PROXYAPI_KEY=your_proxyapi_key_here
PROXYAPI_URL=https://api.proxyapi.com/v1
PROXYAPI_MODEL=gpt-3.5-turbo
PROXYAPI_MAX_TOKENS=1000
PROXYAPI_TEMPERATURE=0.7
PROXYAPI_TIMEOUT=30000

# Global LLM Settings
LLM_MAX_RETRIES=3
LLM_ENABLE_CACHING=true
LLM_ENABLE_LOGGING=true

# Node Environment
NODE_ENV=development